<div class="container mb-5">
  <%= link_to sanitize("&larr;"), root_path, class: "btn btn-blue m-4" %>
  <%= turbo_frame_tag "first_turbo_frame" do %>
    <% if current_user %>
      <div class="header">
        <h1>Happy Things</h1>
      </div>
    <% end %>
  <%= render 'happy_things/happy_thing', happy_things: @happy_things %>
  <%= paginate @happy_things, left: 1, right: 2 %>
  <% end %>
</div>
